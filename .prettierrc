user_input = input("Enter a value: ")

with open('your_text_file.txt', 'r') as file:
    for line in file:
        if user_input in line:
            start = line.find(':') + 1
            end = line.find(':', start)
            if start != -1 and end != -1:
                value = line[start:end]
                print("Found:", value)
                break